@model IEnumerable<PhuDinhData.ReportData.ReportDaily.ReportDailyData>
@{
    ViewBag.Title = "Bao cao hang ngay";
    var grid = new WebGrid(Model, canPage: false, canSort: false);
}

@grid.GetHtml(columns: new[]
{
    grid.Column("TenKho", "Ten Kho", style:"TenKho"),
    grid.Column("TenKhachHang", "Ten Khach Hang", style:"TenKhachHang"),
    grid.Column("SoLuong", "So Luong", style:"SoLuong"),
    grid.Column("TenMatHang", "Ten Mat Hang")
})