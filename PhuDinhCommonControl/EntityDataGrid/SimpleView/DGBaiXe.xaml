<CustomControl:DataGridExt x:Class="PhuDinhCommonControl.EntityDataGrid.DGBaiXe"
                           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                           xmlns:CustomControl="clr-namespace:PhuDinhCommonControl.CustomControl"
                           xmlns:DesignTimeData="clr-namespace:PhuDinhData.DesignTimeData;assembly=PhuDinhData"
                           mc:Ignorable="d"
                           d:DataContext="{x:Static DesignTimeData:DDrBaiXe.rBaiXes}"
                           AutoGenerateColumns="False" EnableRowVirtualization="True" ItemsSource="{Binding}" SelectionMode="Single">
    <DataGrid.Columns>
        <!--Ma-->
        <CustomControl:DataGridReadOnlyTextColumn x:Name="maColumn"/>
        <!--Dia diem bai xe-->
        <CustomControl:DataGridTextColumnExt x:Name="diaDiemBaiXeColumn" Header="Bãi Xe" Binding="{Binding DiaDiemBaiXe}"/>
    </DataGrid.Columns>
</CustomControl:DataGridExt>
